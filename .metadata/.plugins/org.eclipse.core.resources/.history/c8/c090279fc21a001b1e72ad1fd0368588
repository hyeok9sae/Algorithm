import java.util.Scanner;

public class Main_10953 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for (int t = 0; t < T; t++) {
			String s = sc.next();
			for (int i = 0; i < s.length(); i++) {
				int A = s.charAt(0);
				int B = s.charAt(s.length()-1);
				System.out.println((int)A+(int)B);
			}
		}
	}
}
