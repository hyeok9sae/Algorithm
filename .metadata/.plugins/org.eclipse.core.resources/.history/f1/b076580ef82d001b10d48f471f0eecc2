import java.util.Scanner;

public class Main_1463 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[] cnt = {1, 1, 1};
		if (N % 3 == 0) {
			while(true) {
				if (N % 3 == 0) {
					N /= 3;
				} else if (N % 2 == 0) {
					N /= 2;
				} else {
					N -= 1;
				}
				cnt[0]++;
			}
		}
	}
}
