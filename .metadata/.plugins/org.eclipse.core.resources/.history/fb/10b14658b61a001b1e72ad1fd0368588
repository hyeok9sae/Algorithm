import java.util.Scanner;

public class Main_10951 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		while(true) {
			String C = sc.next();
			if(C == "") {
				break;
			}
			char A = C.charAt(0);
			char B = C.charAt(2);
			System.out.println((int)A+(int)B);
		}
	}
}
